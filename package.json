{
  "name": "temp_vite",
  "private": true,
  "version": "0.0.0",
  "main": "dist-electron/main.cjs",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:electron\" \"npm run dev:vite\"",
    "dev:vite": "vite",
    "dev:electron": "tsc -p tsconfig.electron.json && shx mv dist-electron/main.js dist-electron/main.cjs && shx mv dist-electron/preload.js dist-electron/preload.cjs && wait-on tcp:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "build": "tsc -b && vite build && tsc -p tsconfig.electron.json && shx mv dist-electron/main.js dist-electron/main.cjs && shx mv dist-electron/preload.js dist-electron/preload.cjs",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "lucide-react": "^0.562.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^28.2.0",
    "electron-builder": "^26.4.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "rimraf": "^6.1.2",
    "shx": "^0.4.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "wait-on": "^9.0.3"
  }
}
